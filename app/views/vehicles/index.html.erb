<h1>My vehicles</h1>
<div class="container-card">


<% @vehicles.each do |vehicle|%>

  <div class="card" style="width: 18rem;">
    <img src="https://images.unsplash.com/photo-1563460937-8e3fd7f161f6?ixlib=rb-1.2.1&ixid=MnwxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8&auto=format&fit=crop&w=870&q=80" class="card-img-top" alt="...">
    <div class="card-body">
      <h5 class="card-title"><%= vehicle.title %></h5>
      <p class="card-text"><%= vehicle.precio %></p>
      <a href="/vehicles/:id" class="btn btn-primary">Go somewhere</a>

    </div>
  </div>


<% end %>
<a href="/vehicles/new"> + Add a new vehicle </a>
</div>
